{
  "name": "@gasbuddy/configured-swaggerize-express",
  "version": "1.4.0",
  "description": "A configuration driven swaggerize-express middleware",
  "main": "build/index.js",
  "scripts": {
    "test": "tap --node-arg=-r --node-arg=babel-register tests/*.js",
    "test-some": "tap --node-arg=-r --node-arg=babel-register",
    "cover": "cross-env BABEL_ENV=test nyc tap tests/test_*.js && nyc check-coverage",
    "ci-test": "cross-env NODE_ENV=test nyc tap -Rtap tests/test_*.js | tee .tap_output | tap -Rspec -",
    "ci-cover": "nyc check-coverage",
    "lint": "eslint .",
    "build": "babel src --out-dir build",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gas-buddy/gb-services.git/configured-swaggerize-express.git"
  },
  "keywords": [
    "swagger",
    "gasbuddy"
  ],
  "author": "GasBuddy",
  "license": "UNLICENSED",
  "devDependencies": {
    "@gasbuddy/meddleware": "^1",
    "babel-cli": "^6.4.5",
    "babel-preset-gasbuddy": "^2.0.0",
    "bluebird": "^3.2.1",
    "confit": "^2.1.0",
    "cross-env": "^5",
    "eslint": "^3.1.0",
    "eslint-config-gasbuddy": "^2",
    "supertest": "^3",
    "tap": "^10"
  },
  "dependencies": {
    "@gasbuddy/swagger-ref-resolver": "^1.2.0",
    "express": "^4.14.0",
    "swaggerize-express": "^4.0.5"
  },
  "nyc": {
    "lines": 80,
    "statements": 80,
    "functions": 95,
    "branches": 75,
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "src"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
